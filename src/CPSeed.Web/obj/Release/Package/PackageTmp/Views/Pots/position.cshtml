@model IEnumerable<CPSeed.Models.Position>
<div class="col-12 col-md-8 col-lg-3" >
    <div class="item-input-contact">
        @if (ViewBag.Onlinefiling !=1)
        {
            <select name="" id="Career" onchange="Function();" style="background: rgba(193, 195, 184, 0.25);">
                <option value="0" selected="">Tất cả</option>

                @foreach (var item in Model)
                {
                    <option value="@item.Title">@item.Title</option>
                }
            </select>
        }
        else
        {
            <select name="" id="Career">
                <option value="0" selected="">Tất cả</option>

                @foreach (var item in Model)
                {
                    <option value="@item.Title">@item.Title</option>
                }
            </select>
        }
    </div>
</div>
