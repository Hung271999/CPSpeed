
@model IEnumerable<CPSeed.Models.Post>
@using CPSeed.Models;
<div class="list-group list-group-collapse list-group-sm list-group-tree" id="list-group-men" data-children=".sub-men">
    <div class="list-group-collapse sub-men">
        @foreach (var item in Model)
        {
            <a class="list-group-item list-group-item-action dad" href="#@item.PostID" data-toggle="collapse" aria-expanded="true" aria-controls="@item.PostID">
                @item.Title
            </a>
        }
            <a class="list-group-item list-group-item-action dad" href="@Url.Action("Onlinefiling","Pots")" data-toggle="collapse" aria-expanded="true" aria-controls="">
                Nộp hồ sơ trực tuyến
            </a>
    </div>
</div>