@model IEnumerable<CPSeed.Models.Contact>

@{
    Layout = null;
}

<tbody>
    @foreach (var item in Model)
    {
    <tr>
        <td class="tm-product-name">@Html.Raw(item.Title)</td>
        <td class="tm-product-name">@Html.Raw(item.contents)</td>
        <td>
            <a href="@Url.Action("DeleteContact", "Admin" , new { id = item.ContactID})" class="tm-product-delete-link">
                <i class="far fa-trash-alt tm-product-delete-icon"></i>
            </a>
        </td>
        <td>
            <a href="@Url.Action("EditContact", "Admin" , new { id = item.ContactID })">
                Sửa
            </a>
        </td>

    </tr>
    }
</tbody>