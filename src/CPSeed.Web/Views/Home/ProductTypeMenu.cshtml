@model IEnumerable<CPSeed.Models.ProductType>

<li class="dropdown">
    <a href="@Url.Action("Index","Product")" class="nav-link dropdown-toggle arrow" data-toggle="dropdown">Danh mục sản phẩm </a>
    <ul class="dropdown-menu">
        @foreach (var item in Model)
        {
            <li><a href="@Url.Action("Index","Product", new { id = item.ProductTypeID})">@item.ProductTypeName</a></li>
        }
    </ul>
</li>
